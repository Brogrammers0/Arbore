{
  "verbose": true,
  "buildCommand": "npm run package-win && npm run package-linux",
  "pkgFiles": [
    "package.json",
    "app/package.json"
  ],
  "src": {
    "commitMessage": "Release %s-preview",
    "tagName": "v%s-preview"
  },
  "dist": {
    "baseDir": false
  },
  "npm": {
    "private": "true"
  },
  "github": {
    "release": true,
    "preRelease": true,
    "tokenRef": "GITHUB_TOKEN"
  }
}
